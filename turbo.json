{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "env": ["DATABASE_URL"]
    },
    "@director.run/cli#build": {
      "dependsOn": ["^build", "@director.run/studio#build"],
      "inputs": ["$TURBO_DEFAULT$"],
      "outputs": ["dist/**"],
      "env": ["DATABASE_URL"]
    },
    "dev": {
      "inputs": ["$TURBO_DEFAULT$"],
      "persistent": true,
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "format": {
      "dependsOn": ["^format"]
    },
    "start": {
      "persistent": true,
      "cache": false
    },
    "clean": {
      "dependsOn": ["^clean"]
    },
    "test": {
      "cache": false
    },
    "release": {
      "cache": false,
      "env": ["NPM_CONFIG_TOKEN"]
    }
  }
}
