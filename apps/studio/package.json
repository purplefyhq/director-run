{
  "name": "@director.run/studio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "bun run clean && BASE_PATH=/studio vite build",
    "preview": "vite preview",
    "serve": "bun run server/index.ts",
    "clean": "rm -rf dist",
    "start": "bun run build && BASE_PATH=/studio bun run serve",
    "typecheck": "tsc --noEmit",
    "lint": "biome check ./src && biome check ./server",
    "format": "biome format --write ./src && biome format --write ./server"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^5.0.2",
    "typescript": "^5.8.3",
    "vite": "^7.1.5"
  },
  "dependencies": {
    "@director.run/design": "workspace:*",
    "@director.run/utilities": "workspace:*",
    "@trpc/next": "^11.1.3",
    "@trpc/react-query": "^11.1.3",
    "@trpc/tanstack-react-query": "^11.1.3",
    "express": "^4.21.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.0.0"
  }
}
