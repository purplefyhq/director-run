{
  "proxies": [
    {
      "name": "test-proxy",
      "servers": [
        {
          "name": "Hackernews",
          "transport": {
            "command": "uvx",
            "args": [
              "--from",
              "git+https://github.com/erithwik/mcp-hn",
              "mcp-hn"
            ]
          }
        },
        {
          "name": "Fetch",
          "transport": {
            "command": "uvx",
            "args": ["mcp-server-fetch"]
          }
        },
        {
          "name": "test-sse-transport",
          "transport": {
            "type": "sse",
            "url": "http://localhost:4521/sse"
          }
        }
      ]
    }
  ]
}
